  15 |         this.page = page;
  16 |         this.header = new Header(this.page);
  17 |
  18 |         this.logo = this.page.getByAltText('Website for automation practice', {exact: true});
  19 |     };
  20 |
  21 |     async navigateToHomePage() {
  22 |         await this.page.goto(this.url);
  23 |     }
  24 |
  25 |     async clickLogin() {
  26 |         await this.header.clickLogin();
  27 |     }
  28 |
  29 |     async verifyUserIsLoggedIn() {
  30 |         await test.step('Verify User Is Logged In', async () => {
  31 |             await this.header.verifyLogoutLinkDisplays();
  32 |         })
  33 |     }
  34 |
  35 |     async clickProductsLink() {
  36 |         await this.header.clickProductsLink();
